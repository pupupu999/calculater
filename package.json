{
  "name": "poker-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": "./",
  "scripts": {
    "dev": "node server/server.js",
    "start": "NODE_ENV=production node server/server.js",
    "start-client": "cd client/poker-app && HOST=0.0.0.0 PORT=3000 react-scripts start",
    "start-all": "concurrently --names \"server,client\" -c \"blue,green\" \"node server/server.js\" \"cd client/poker-app && HOST=0.0.0.0 PORT=3000 react-scripts start\""
  },
  "dependencies": {
    "@babel/runtime": "^7.25.6",
    "@craco/craco": "^7.1.0",
    "antd": "^5.24.5",
    "babel-plugin-styled-components": "^2.1.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "firebase": "^10.14.0",
    "firebase-admin": "^13.2.0",
    "helmet": "^8.1.0",
    "igniteui-react-charts": "^18.7.3",
    "igniteui-react-core": "^18.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^5.0.1",
    "recharts": "^2.13.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "concurrently": "^9.1.2",
    "eslint": "^8"
  }
}
